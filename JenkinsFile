pipeline {
	agent any
	stages {
		stage('checkout'){
			steps {
				git 'https://github.com/nigelbrennan/CICDproject.git'
			}
		}
		stage('Build'){
			steps {
				bat "mvn clean compile"
			}
		}
		stage('test'){
			steps {
			 	bat "mvn test"
			 	junit '**/target/surefire-reports/TEST-*.xml'
			 }
		}
		stage('Package') {
			steps {
				bat "mvn package"
			}
		}		
	}
}
				